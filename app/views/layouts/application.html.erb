<!DOCTYPE html>
<html>
  <head>
    <title>App</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  

  <body>
    <nav id="navbar-menu" class="navbar navbar-default navbar-static-top navbar-fixed-top">
      <div class="container-fluid">
        <div class="navbar-header">
          <%= link_to image_tag("Titulo.png", alt: "BIOENERGY", id:"logo-bioenergy"), :root, class:"navbar-brand" %>
          <!--%= image_tag "Titulo.png", alt: "", style:"height: 80%; width:80%", class:"" %-->
        </div>
      </div>
    </nav>
  
    <%= yield %>
  </body>
</html>
